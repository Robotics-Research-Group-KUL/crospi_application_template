<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robelix_neura_robot" >

  <xacro:arg name="prefix" default="maira7M_"/>

  <xacro:property name="prefix" value="maira7M_" />

  <xacro:arg name="robot_ns" default="mav" />


    <xacro:include filename="$(find mav_description)/urdf/mav/mav_plus_macro.xacro" />


	<xacro:mav_plus axes_config="fixed" gripper="" arm_name="maira7M"  
            backpack="" lin_axis_base_x="0.52" lin_axis_base_z="0.8125" lin_axis_height="0.345"  
            robot_base_yaw="-0.785398" camera_setup="link6" complex_mav_urdf="true" fixed_wheels="true"
            klt_box="false" small_klt_box="false"/>


	<link name="world" />
	

    <joint name="world_joint_mav" type="fixed">
        <parent link="world" />
        <child link = "$(arg robot_ns)_base_link" />
        <origin xyz="$(arg tx) $(arg ty) $(arg tz)" rpy="$(arg rr) $(arg rp) $(arg ry)" />
    </joint>

    <link name="FT_sensor_frame" />
	<joint name="FT_sensor_frame_joint" type="fixed">
		<parent link="$(arg prefix)flange" />
		<child link = "FT_sensor_frame" />
		<origin xyz="0.0 0.0 0.066" rpy="0.0 0.0 0.0" />
	</joint>

    <link name="tool0_grip" />
	<joint name="tool0_grip_joint" type="fixed">
		<parent link="FT_sensor_frame" />
		<child link = "tool0_grip" />
		<origin xyz="0.0 0.0 0.172" rpy="0.0 3.1416 0.0" />
	</joint>

	<link name="tool0_grip_rot" />
	<joint name="tool0_grip_rot_joint" type="fixed">
		<parent link="tool0_grip" />
		<child link = "tool0_grip_rot" />
		<origin xyz="0.0 0.0 0.0" rpy="0.0 -1.0472 0.0" />
	</joint>

	<link name="tool0_insertions" />
	<joint name="tool0_insertions_joint" type="fixed">
		<parent link="tool0_grip_rot" />
		<child link = "tool0_insertions" />
		<origin xyz="0.0 0.0 -0.085" rpy="0.0 0.0 -1.5708" />
	</joint>


    <link name="tool0_cables" />
	<joint name="tool0_cables_joint" type="fixed">
		<parent link="FT_sensor_frame" />
		<child link = "tool0_cables" />
		<origin xyz="0.0 0.0 0.132" rpy="0.0 3.1416 0.0" />
	</joint>


</robot>
