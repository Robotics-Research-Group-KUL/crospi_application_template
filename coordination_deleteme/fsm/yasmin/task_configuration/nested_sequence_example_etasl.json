{
    "$schema": "../../../../etasl/tasks-schema.json",
    "tasks": [
        {
            "name": "MovingHome",
            "robot_specification_file": "$[etasl_ros2_application_template]/etasl/robot_specifications/ur10.etasl.lua",
            "task_specification": {
                "is-move_jointspace_trap": true,
                "file_path": "$[etasl_ros2_application_template]/etasl/task_specifications/move_jointspace_trap.etasl.lua",
                "parameters": {
                    "maxacc": 0.1,
                    "maxvel": 0.1,
                    "target_joints": [3.0416, -1.5708, 1.3, -1.3, -1.2, 0],
                    "units": "radians"
                }
            }
        },
        {
            "name": "MovingCartesian",
            "task_specification": {
                "is-move_cartesianspace": true,
                "file_path": "$[etasl_ros2_application_template]/etasl/task_specifications/move_cartesianspace.etasl.lua",
                "parameters": {
                    "delta_pos": [0.0, 0.0, 0.1],
                    "eq_r": 0.08,
                    "maxacc": 0.1,
                    "maxvel": 0.1
                }
            }
        },
        {
            "name": "MovingJoystick",
            "robot_specification_file": "$[etasl_ros2_application_template]/etasl/robot_specifications/ur10.etasl.lua",
            "task_specification": {
                "is-move_joystick": true,
                "file_path": "$[etasl_ros2_application_template]/etasl/task_specifications/move_joystick.etasl.lua",
                "parameters": {
                    "angular_scale": 1,
                    "linear_scale": 1
                }
            }
        },
        {
            "name": "MovingDown",
            "robot_specification_file": "$[etasl_ros2_application_template]/etasl/robot_specifications/ur10.etasl.lua",
            "task_specification": {
                "is-move_cartesianspace": true,
                "file_path": "$[etasl_ros2_application_template]/etasl/task_specifications/move_cartesianspace.etasl.lua",
                "parameters": {
                    "delta_pos": [0, 0, -0.2],
                    "eq_r": 0.08,
                    "maxacc": "$blackboard/output_param/MovingUp/maxacc",
                    "maxvel": 0.1
                }
            }
        },
        {
            "name": "MovingUp",
            "robot_specification_file": "$[etasl_ros2_application_template]/etasl/robot_specifications/ur10.etasl.lua",
            "task_specification": {
                "is-move_cartesianspace": true,
                "file_path": "$[etasl_ros2_application_template]/etasl/task_specifications/move_cartesianspace.etasl.lua",
                "parameters": {
                    "delta_pos": [0, 0, 0.2],
                    "eq_r": 0.08,
                    "maxacc": 0.1,
                    "maxvel": 0.1
                }
            }
        }
    ]
}