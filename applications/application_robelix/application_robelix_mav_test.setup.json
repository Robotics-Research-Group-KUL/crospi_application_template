{
    "$schema": "../../schemas/generated/blackboard-schema-bundled.json",
    "blackboard": {
        "etasl": {
            "general": {
                "event_topic": "etasl/events",
                "sample_time": 0.01
            },
            "initializer": {
                "full": true,
                "sample_time": 0.01,
                "duration": 3.0,
                "convergence_criterion": 1E-8,
                "weightfactor": 1E4
            },
            "solver": {
                "is-qpoasessolver": true,
                "nWSR": 100,
                "regularization_factor": 1E-8,
                "cputime": 10
            }
        },
        "robot": {
            "robotsimulators": [
                {
                    "is-simple_kinematic_simulation": true,
                    "initial_joints": [0.3, 0.483, 0, 1.15, 0, 1.519, 0],
                    "periodicity": 0.01,
                    "robot_joints": [
                        "maira7M_joint1",
                        "maira7M_joint2",
                        "maira7M_joint3",
                        "maira7M_joint4",
                        "maira7M_joint5",
                        "maira7M_joint6",
                        "maira7M_joint7"
                    ]
                },
                {
                    "is-simple_kinematic_simulation": true,
                    "initial_joints": [0, 0, 0],
                    "periodicity": 0.01,
                    "robot_joints": [
                        "x_mobile_joint",
                        "y_mobile_joint",
                        "theta_mobile_joint"
                    ]
                }
            ],
            "robotdrivers": [
                {
                    "is-neura_manipulator_driver_etasl": true,
                    "periodicity": 0.001,
                    "robot_model": "Maira",
                    "serial_number": 227080,
                    "robot_joints": [
                        "maira7M_joint1",
                        "maira7M_joint2",
                        "maira7M_joint3",
                        "maira7M_joint4",
                        "maira7M_joint5",
                        "maira7M_joint6",
                        "maira7M_joint7"
                    ]
                },
                {
                    "is-mav_driver_etasl": true,
                    "periodicity": 0.01,
                    "command_topic": "/mav1500/twist_cmd",
                    "feedback_topic": "/mav1500/pose",
                    "robot_joints": [
                        "x_mobile_joint",
                        "y_mobile_joint",
                        "theta_mobile_joint"
                    ]
                }
            ],
            "default_robot_specification": {
                "is-lua_robotspecification": true,
                "file_path": "$[etasl_ros2_application_template]/robot_models/robot_specifications/robelix_neura.etasl.lua",
                "robot_joints": [
                    "x_mobile_joint",
                    "y_mobile_joint",
                    "theta_mobile_joint",
                    "maira7M_joint1",
                    "maira7M_joint2",
                    "maira7M_joint3",
                    "maira7M_joint4",
                    "maira7M_joint5",
                    "maira7M_joint6",
                    "maira7M_joint7"
                ]
            }
        },
        "iohandlers": {
                "outputhandlers": [
                    {
                        "is-JointStateOutputHandler": true,
                        "topic-name": "/joint_states"
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/my_topic",
                        "variable-names" : []
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/wrench_output",
                        "variable-names" : ["Fx", "Fy", "Fz", "Tx", "Ty", "Tz"]
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/angle_output",
                        "variable-names" : ["angle"]
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/etasl/tracking_error",
                        "variable-names" : ["tracking_error_j1","tracking_error_j2","tracking_error_j3", "tracking_error_j4", "tracking_error_j5", "tracking_error_j6", "tracking_error_j7"]
                    },
                    {
                        "is-TopicOutputHandler": true,
                        "topic-name": "/etasl/tcp_position",
                        "variable-names": ["x_tf", "y_tf", "z_tf"]
                    }
                ],
                "inputhandlers": [
                    {
                        "is-TwistInputHandler": true,
                        "default_twist": {"linear": {"x": 0, "y": 0, "z": 0},"angular": {"x": 0, "y": 0, "z": 0}},
                        "depth": 1,
                        "number_of_tries": 10,
                        "topic-name": "/spacenav/twist",
                        "when_unpublished": "use_default",
                        "varname": "joystick_input"
                    },
                                        {
                        "is-WrenchInputHandler": true,
                        "topic-name": "/schunk/wrench",
                        "number_of_tries": 10,
                        "depth": 1,
                        "default_wrench": {"force": {"x": 0, "y": 0, "z": 0},"torque": {"x": 0, "y": 0, "z": 0}},
                        "when_unpublished": "throw_error",
                        "varname": "wrench_input"
                    },
                    {
                        "is-PoseInputHandler": true,
                        "number_of_tries": 10,
                        "depth": 1,
                        "topic-name": "/charuco_detector/pose",
                        "when_unpublished": "throw_error",
                        "varname": "board_pose",
                        "default_pose": {
                            "orientation":{
                                "is-rpy": true,
                                "pitch": 0,
                                "roll": 0,
                                "yaw": 0
                            },
                            "origin": {
                                "x": 0,
                                "y": 0,
                                "z": 0
                            }
                        }
                    }
                ]
            }
    },
    "$defs":{}
}
