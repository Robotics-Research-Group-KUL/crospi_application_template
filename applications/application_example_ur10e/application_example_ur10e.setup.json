{
    "$schema": "../../schemas/generated/blackboard-schema-bundled.json",
    "blackboard": {
        "etasl": {
            "general": {
                "event_topic": "etasl/events",
                "sample_time": 0.01
            },
            "initializer": {
                "full": true,
                "sample_time": 0.01,
                "duration": 3.0,
                "convergence_criterion": 1E-8,
                "weightfactor": 1E4
            },
            "solver": {
                "is-qpoasessolver": true,
                "nWSR": 100,
                "regularization_factor": 1E-8,
                "cputime": 10
            }
        },
        "robot": {
            "robotsimulator": {
                "is-simple_kinematic_simulation": true,
                "initial_joints": [1.5708, -1.5708, 2.0944, -2.0944, -1.5708, 0.2],
                "periodicity": 0.01
            },
            "robotdriver":{
                "is-ur10_e_driver_etasl": true,
                "ip_address": "192.168.56.102",
                "periodicity": 0.02,
                "q_acceleration": 1.2
            },
            "default_robot_specification": {
                "is-inline_robotspecification": true,
                "urdf_path": "$[etasl_ros2_application_template]/robot_models/urdf_models/robot_setups/ur10e/use_case_setup_ur10e.urdf",
                "tcp_frame": {
                    "child_link": "tool0",
                    "parent_link": "base_link"
                },
                "additional_frames": [
                    {
                        "name": "FT_sensor_frame",
                        "child_link": "ft_frame",
                        "parent_link": "base_link"
                    }
                ],
                "robot_joints": [
                    "shoulder_pan_joint",
                    "shoulder_lift_joint",
                    "elbow_joint",
                    "wrist_1_joint",
                    "wrist_2_joint",
                    "wrist_3_joint"
                ]
            }
        },
        "iohandlers": {
                "outputhandlers": [
                    {
                        "is-JointStateOutputHandler": true,
                        "topic-name": "/joint_states"
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/my_topic",
                        "variable-names" : []
                    },
                    {
                        "is-TopicOutputHandler" : true,
                        "topic-name" : "/forces_etasl",
                        "variable-names" : ["x_force", "y_force", "z_force"]
                    }
                ],
                "inputhandlers": [
                    {
                        "is-TwistInputHandler": true,
                        "default_twist": {"linear": {"x": 0, "y": 0, "z": 0},"angular": {"x": 0, "y": 0, "z": 0}},
                        "depth": 1,
                        "number_of_tries": 10,
                        "topic-name": "/spacenav/twist",
                        "when_unpublished": "use_default",
                        "varname": "joystick_input"
                    },
                    {
                        "is-PoseInputHandler": true,
                        "number_of_tries": 10,
                        "feedforward_input_topic": "/etasl/feedforward/twist_of_tf",
                        "depth": 1,
                        "topic-name": "/tf_to_follow",
                        "when_unpublished": "use_default",
                        "varname": "board_pose",
                        "default_pose": {
                            "orientation":{
                                "is-rpy": true,
                                "pitch": 0,
                                "roll": 0,
                                "yaw": 0
                            },
                            "origin": {
                                "x": 0,
                                "y": 0,
                                "z": 0
                            }
                        }
                    }
                ]
            }
    },
    "$defs":{}
}
